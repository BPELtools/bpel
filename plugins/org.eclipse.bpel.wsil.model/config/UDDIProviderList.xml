<?xml version="1.0" encoding="UTF-8"?>
<providerList>
	<provider>
		<description>Local BPEL Server</description>
		<type>wsil</type>
        <inquiryURL>http://mchmiele-pc.us.oracle.com:9700/BPELConsole/xmlGetProcessList.jsp?domainId=default&amp;password=bpel</inquiryURL>
		<inquiryURL>http://[HTTP_HOST]:[HTTP_PORT]/BPELConsole/xmlGetProcessList.jsp?domainId=default&amp;password=bpel</inquiryURL>
	</provider>

	<provider>
		<description>Root WSIL Document</description>
		<type>wsil</type>
		<inquiryURL>root.wsil.xml</inquiryURL>
	</provider>

		
	<provider>
		<description>Local XMethods Bookmarks</description>
		<type>wsil</type>
		<inquiryURL>xmethodsBookmark.xml</inquiryURL>
	</provider>	

	<provider>
		<description>XMethods Directory (Cached)</description>
		<type>wsil</type>
		<inquiryURL>xmethods.cached.wsil.xml</inquiryURL>
	</provider>	
	
	<provider>
		<description>*Live* XMethods Directory</description>
		<type>wsil</type>
		<inquiryURL>http://www.xmethods.net/inspection.wsil</inquiryURL>
	</provider>	

	<provider>
		<description>Built in</description>
		<type>wsil</type>
		<inquiryURL>builtinServices.xml</inquiryURL>
	</provider>	
	
	<!--
	this assumes that you have deployed the oracle UDDIBrowser application (war)
	on the local server (see gateway attribute) and that you have the Systinet WASP
	UDDI server running on port 8080 
	<provider>
		<description>Systinet WASP UDDI Server</description>
		<type>uddi</type>
		<inquiryURL>http://localhost:8080/uddi/inquiry</inquiryURL>
		<gateway>http://localhost:9700/UDDIBrowser/xmlGetUDDI.jsp</gateway>
	</provider>
	-->
	<!--
	this assumes that you have deployed the oracle UDDIBrowser application (war)
	on the local server (see gateway attribute) and that you have the WebLogic
	UDDI listener configured and running on port 7001 
	<provider>
		<type>uddi</type>
		<description>Weblogic UDDI Server</description>
		<inquiryURL>http://localhost:7001/uddi/uddilistener</inquiryURL>
		<gateway>http://localhost:9700/UDDIBrowser/xmlGetUDDI.jsp</gateway>
	</provider>	
	-->
</providerList>
